   M'inscrire
   var popinNLPath =
   "http://suite11.emarsys.net/u/register.php?CID=547292539&f=1457&p=2&a=r
   &SID=&el=&llid=&counted=&c=&optin=y&key_id=3&interest[]=[Interessen]&in
   p_3="; function subscribe(){ var email =
   document.getElementById("spr-email").value; if(checkMail(email)){
   popinNLPath += email; popinNLPath += "&inp_11993=2";
   document.getElementById("popin-request").src = popinNLPath;
   document.getElementById("popin-nl-spr-button").setAttribute('onclick',
   ''); document.getElementById("popin-nl-spr-button").innerHTML =
   "Patientez..." setTimeout(function(){
   document.getElementById("popin-nl").innerHTML = '
   Votre inscription a bien été prise en compte
   '}, 5000); }else{ alert('Veuillez renseigner une adresse email
   valide.'); return false; } } function checkMail(c){var a=new
   RegExp("^[a-z0-9]+([_|.|-]{1}[a-z0-9]+)*@[a-z0-9]+([_|.|-]{1}[a-z0-9]+)
   *[.]{1}[a-z]{2,6}$","i");if(!a.test(c)){return false}else{return true}}

