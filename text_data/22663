   Remplacement des \ par des / longdescUrls = longdescUrls.replace(/\\/g,
   '\/'); // Cration de l'array contenant les url de longdesc
   arrLongdescUrls = longdescUrls.split(/\|/); } if(alternateImgs != '') {
   // Remplacement des \ par des / alternateImgs =
   alternateImgs.replace(/\\/g, '\/'); // Cration de l'array contenant les
   images alternatives arrAlternateImg = alternateImgs.split(/\|/);
