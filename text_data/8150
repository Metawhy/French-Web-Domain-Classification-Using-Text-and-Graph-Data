   function SetSmileys(a){ var i = document.getElementById("ChatReponse");
   var v = a.dataset.smiley; i.focus(); if(document.selection){ i.focus();
   s = document.selection.createRange(); s.text = v; i.focus(); }else
   if(i.selectionStart || i.selectionStart == '0'){ var startPos =
   i.selectionStart; var endPos = i.selectionEnd; var scrollTop =
   i.scrollTop; i.value = i.value.substring(0,
   startPos)+v+i.value.substring(endPos,i.value.length); i.focus();
   i.selectionStart = startPos + v.length; i.selectionEnd = startPos +
   v.length; i.scrollTop = scrollTop; }else{ i.value += v; i.focus(); } }
   function ApercuLienBurger(i,e){ var a =
   document.getElementById("ApercuMenuBurger"); if(e){ a.innerHTML = "
   [\""+i.src+"\"] " + i.dataset.titre + "
   " + i.dataset.info; }else{ a.innerHTML = ""; } } function
   ApercuSmiley(i,e){ var a = document.getElementById("ApercuSmiley");
