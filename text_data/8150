   i.files[0].type; var file = new FileReader; file.onload = function(){
   if(typeof this.result=="string"){ var image = new Image; image.onload =
   function(){ var MaxWidth = 600; var MaxHeight = 600;
   if(this.width<=MaxWidth && this.height<=MaxHeight){ var width =
   this.width; var height = this.height; }else{ if(this.widthMaxHeight){
   var width = Math.round(this.width * MaxHeight / this.height); var
   height = MaxHeight; }else{ var width = MaxWidth; var height =
   Math.round(this.height * MaxWidth / this.width); } }else{// Mode
   paysage if(this.width>MaxWidth){ var width = MaxWidth; var height =
   Math.round(this.height * MaxWidth / this.width); }else{ var width =
   Math.round(this.width * MaxHeight / this.height); var height =
   MaxHeight; } } } var canvas = document.createElement("canvas");
   canvas.width = width; canvas.height = height;
   canvas.getContext("2d").drawImage(this,0,0,this.width,this.height,0,0,w
   idth,height); var data = canvas.toDataURL("image/jpeg"); delete canvas;
   if(data.length<=2097152){
   document.getElementById("ChatImageContenu").value = data; }else{
   monThis.effaceImage("Veuillez ne pas dépasser 2Mo, merci."); } }
   image.src = this.result; } } file.onabort =
   function(e){alert("Interruption de l'opération");} file.onerror =
   function(e){alert("Erreur : " + (this.result || e));}
   file.readAsDataURL(i.files[0]); }else{ monThis.effaceImage("Le type de
