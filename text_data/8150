   switch(i.files[0].type){ case
   'application/vnd.openxmlformats-officedocument.wordprocessingml.documen
   t':ext='docx';break; case 'application/msword':ext='doc';break; case
   'application/pdf':ext='pdf';break; case 'application/binary':
   if(i.files[0].name.indexOf('.pdf')) ext='pdf'; else
   if(i.files[0].name.indexOf('.doc')) ext='doc'; else
   if(i.files[0].name.indexOf('.docx')) ext='docx'; break; } if(ext){
   document.getElementById("ChatFichierNom").value = i.files[0].name;
   document.getElementById("ChatFichierType").value = i.files[0].type; var
   file = new FileReader; file.onload = function(){ if(typeof
   this.result=="string"){ if(this.result.length<=2097152){
   document.getElementById("ChatFichierContenu").value = this.result;
   }else{ monThis.effaceFichier("Veuillez ne pas dépasser 2Mo, merci."); }
   } } file.onabort = function(e){alert("Interruption de l'opération");}
   file.onerror = function(e){alert("Erreur : " + (this.result || e));}
   file.readAsDataURL(i.files[0]); }else{ monThis.effaceFichier("Le type
   de votre fichier "+i.files[0].type+" n'est pas autorisé."); } }); }
