   var LastSaisi = null; function
   EnvoiReponse(t,IDElement,IDAttachement,Num,Saisi){ LastSaisi = Saisi;
   var iSaisi = null; var ChatChoixMultiples = ""; if(Saisi==5){ var cx =
   document.querySelectorAll("input.chatChoixMultiples"); var valeurs =
   ""; for(var i=0;i0){ if(Saisi==100){
   document.getElementById("ChatReponse").value =
   t.getElementsByTagName('span')[0].innerHTML; Saisi=1; } if(Saisi==1 &&
   document.getElementById("ChatReponse")) iSaisi =
   document.getElementById("ChatReponse"); else if(Saisi==2 &&
   document.getElementById("ChatDate")) iSaisi =
   document.getElementById("ChatDate"); else if(Saisi==3 &&
   document.getElementById("ChatHeure")) iSaisi =
   document.getElementById("ChatHeure"); else if(Saisi==4 &&
   document.getElementById("ChatCouleur")) iSaisi =
   document.getElementById("ChatCouleur"); else if(Saisi==10 &&
   document.getElementById("ChatPave")) iSaisi =
   document.getElementById("ChatPave"); } if(iSaisi &&
   !iSaisi.value.trim()){ alert("Vous n’avez rien saisi !"); return false;
   } if(Saisi==101){ iSaisi = t.getElementsByTagName('input')[0]; Saisi =
   1; } if(t && t.dataset.sauve){ if(iSaisi)
   localStorage.setItem(t.dataset.sauve,iSaisi.value.trim()); else
   localStorage.setItem(t.dataset.sauve,t.textContent); } } var data =
   "IDElement="+IDElement+"&IDAttachement="+IDAttachement+"&Num="+Num+"&Je
   ton="+document.getElementById("kwalys-form-jeton").value;
   if(document.getElementById("kwalys-form-id")) data +=
   "&ID="+encodeURI(document.getElementById("kwalys-form-id").value);
   if(iSaisi) data += "&ChatReponse="+encodeURI(iSaisi.value);
   if(Saisi==12){ data += "&ChatReponse="+encodeURI(t.innerHTML); }
   if(Saisi==14){ let ref = KwalysChatbot.Diode.getreference(); let value
   = KwalysChatbot.Diode.getvalue(); data += "&ChatReponse="+encodeURI(ref
   + value); } if(ChatChoixMultiples) data +=
   "&ChatChoixMultiples="+encodeURI(ChatChoixMultiples);
   if(document.getElementById("CodeSVG")){ data +=
   "&CodeSVG="+encodeURI(document.getElementById("CodeSVG").value); data
   +=
   "&CanvasW="+encodeURI(document.getElementById("CanvasSignature").client
   Width); data +=
   "&CanvasH="+encodeURI(document.getElementById("CanvasSignature").client
   Height); } KwalysChatbot.envoiReponse(data); return false; } function
   CatalogueSuite(t,IDRubrique,IDElement,IDAttachement){ var data =
   "IDRubrique="+IDRubrique+"&IDElement="+IDElement+"&IDAttachement="+IDAt
   tachement+"&Jeton="+document.getElementById("kwalys-form-jeton").value;
   if(document.getElementById("kwalys-form-id")) data +=
   "&ID="+encodeURI(document.getElementById("kwalys-form-id").value);
   KwalysChatbot.envoiReponse(data); return false; } function
   CataloguePanier(t,IDProduit,IDElement,IDAttachement){ var data =
   "IDProduit="+IDProduit+"&IDElement="+IDElement+"&IDAttachement="+IDAtta
   chement+"&Jeton="+document.getElementById("kwalys-form-jeton").value;
   if(document.getElementById("kwalys-form-id")) data +=
   "&ID="+encodeURI(document.getElementById("kwalys-form-id").value);
   KwalysChatbot.envoiReponse(data); return false; } function removeEmojis
   (string) { var regex =
   /(?:[\u2700-\u27bf]|(?:\ud83c[\udde6-\uddff]){2}|[\ud800-\udbff][\udc00
   -\udfff]|[\u0023-\u0039]\ufe0f?\u20e3|\u3299|\u3297|\u303d|\u3030|\u24c
   2|\ud83c[\udd70-\udd71]|\ud83c[\udd7e-\udd7f]|\ud83c\udd8e|\ud83c[\udd9
   1-\udd9a]|\ud83c[\udde6-\uddff]|\ud83c[\ude01-\ude02]|\ud83c\ude1a|\ud8
   3c\ude2f|\ud83c[\ude32-\ude3a]|\ud83c[\ude50-\ude51]|\u203c|\u2049|[\u2
   5aa-\u25ab]|\u25b6|\u25c0|[\u25fb-\u25fe]|\u00a9|\u00ae|\u2122|\u2139|\
   ud83c\udc04|[\u2600-\u26FF]|\u2b05|\u2b06|\u2b07|\u2b1b|\u2b1c|\u2b50|\
   u2b55|\u231a|\u231b|\u2328|\u23cf|[\u23e9-\u23f3]|[\u23f8-\u23fa]|\ud83
   c\udccf|\u2934|\u2935|[\u2190-\u21ff])/g; return string.replace(regex,
   ''); } function GoLecture(Texte){ if(KwalysChatbot.lectureSon){ var
   synth = window.speechSynthesis; if(synth){ var utterThis = new
   SpeechSynthesisUtterance(removeEmojis(Texte)); synth.speak(utterThis);
   } } } function EnvoiReponse13(t,g,event,IDElement,IDAttachement,Num){
   if(!event && window.event) event=window.event; if(event.keyCode){
   if(event.keyCode==13) return
   EnvoiReponse(t,IDElement,IDAttachement,Num,true); if(g==1 &&
   event.keyCode != 8 && event.keyCode != 45 && event.keyCode != 46 &&
   (event.keyCode < 48 || event.keyCode > 57)){ event.returnValue = false;
   event.cancelBubble = true; return false; } } if(event.which){
   if(event.which==13) return
   EnvoiReponse(t,IDElement,IDAttachement,Num,true); if(g==1 &&
   event.which != 8 && event.which != 45 && event.which != 46 &&
   (event.which < 48 || event.which > 57)){ event.preventDefault();
   event.stopPropagation(); return false; } } return true; } function
   EnvoiReponseFichier(IDElement,IDAttachement,Num){
   if(document.getElementById("ChatFichierContenu")){
   if(!document.getElementById("ChatFichierContenu").value.trim()){
   alert("Vous n’avez pas entré le fichier !"); }else{ var data =
   "IDElement="+IDElement+"&IDAttachement="+IDAttachement+"&Num="+Num+"&Je
   ton="+document.getElementById("kwalys-form-jeton").value;
   if(document.getElementById("kwalys-form-id")) data +=
   "&ID="+encodeURI(document.getElementById("kwalys-form-id").value);
   if(document.getElementById("ChatFichierNom")) data +=
   "&ChatFichierNom="+encodeURI(document.getElementById("ChatFichierNom").
   value); if(document.getElementById("ChatFichierType")) data +=
   "&ChatFichierType="+encodeURI(document.getElementById("ChatFichierType"
   ).value); if(document.getElementById("ChatFichierContenu")) data +=
   "&ChatFichierContenu="+encodeURI(document.getElementById("ChatFichierCo
   ntenu").value); KwalysChatbot.envoiReponse(data); } } return false; }
