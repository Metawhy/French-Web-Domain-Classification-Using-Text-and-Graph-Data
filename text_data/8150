   " + i.dataset.smiley; }else{ a.innerHTML = ""; } } function
   PaveNumerique(n){ if(document.getElementById("ChatPave")){
   if(n=="Supprimer"){ var v = document.getElementById("ChatPave").value;
   document.getElementById("ChatPave").value = v.substr(0,v.length-1);
   }else document.getElementById("ChatPave").value += n; } } function
