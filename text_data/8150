   KwalysChatbot = new KwalysChatbot();
   document.addEventListener("DOMContentLoaded",function(event){KwalysChat
   bot.Demarrer();}); function signaturesvg(conf){ var drawCtx =
   {strokeStyle:"#000000",lineWidth:1,lineCap:"round",lineJoin:"round"};
   var defaultConf = {id:"canvas",width:300,height:200}; var coords = [];
   var conf = merge(conf,defaultConf); var canvas; var context; var
   mouseReleased = true; function merge(a,b,filter){ var t = {};
   mergeOn(t,a,b,filter); return t; } function mergeOn(t,a,b,filter){ var
   done = {}; b = b === undefined ? {} : b; var f; if((Array.isArray &&
   Array.isArray(filter)) || (typeof filter === 'object' && typeof
   filter.length === 'number')){ f = function(o){ for(var i = 0; i <
   filter.length; i++){ if( o === filter[i] ){ return true; }; } return
   false; }; } else if('function' === typeof filter){ f = filter; } else
   if('object' === typeof filter){ f = function(o){ return
   filter.hasOwnProperty(o); }; } else if(filter === undefined || filter
   === null){ f = function(o){ return true; }; } else{ throw new
   TypeError("illegal filter type [" + typeof filter + "]"); } for(var i
   in a){ if(!f(i)) continue; var v1 = a[i]; if(v1 === undefined){ var v2
   = b[i]; if(v2 !== undefined) t[i] = v2; }else t[i] = v1; done[i] =
   true; } for(var i in b){ if(done[i] || !f(i)) continue; t[i] = b[i]; }
   return t; } function findElement(selector){ var elt; if(typeof selector
   === 'string'){ if(document.querySelector){ elt =
   document.querySelector(selector); } else
   if(/^[.][A-Za-z0-9_-]+$/.match(selector)){ elt =
   document.getElementsByClassName(selector.startsWith('.') ?
   selector.substring(1) : selector)[0]; } else{ elt =
   document.getElementById(selector.startsWith('#') ?
   selector.substring(1) : selector); } } else if(typeof selector ===
   'function'){ elt = selector(); } else if(typeof selector === 'object'){
   elt = selector; } return elt; } this.clear = function(){
   context.closePath(); context.clearRect(0,0,conf.width,conf.width);
   context.beginPath(); mouseReleased = true; coords = []; } function
   addCoords(x,y,started){ var data; if(started){ data =
   [x,y,merge(drawCtx)]; context.moveTo(x,y); } else{ data = [x,y];
   context.lineTo(x,y); context.stroke(); } coords.push(data); } function
   endDraw(){ if(coords.length===0) return; var xy = coords[coords.length
   - 1]; if(xy.length > 2){ context.lineTo(xy[0],xy[1]); context.stroke();
   } } function createListeners(){ function getXY(obj){ var x=0,y=0;
   while(obj!=null){ x+=obj.offsetLeft-obj.scrollLeft;
   y+=obj.offsetTop-obj.scrollTop; obj=obj.offsetParent; } return
   {x:x,y:y}; } function getCoords(e,t){ var ws =
   getXY(document.getElementById(conf.id)); return { x:(e.changedTouches ?
   e.changedTouches[0].pageX : e.pageX) - ws.x - window.scrollX,
   y:(e.changedTouches ? e.changedTouches[0].pageY : e.pageY) - ws.y -
   window.scrollY }; } function press(e){ var coords = getCoords(e,this);
   mouseReleased = false; addCoords(coords.x,coords.y,true); } function
   drag(e){ if(!mouseReleased){ var coords = getCoords(e,this);
   addCoords(coords.x,coords.y,false); } e.preventDefault(); } function
   release(){ mouseReleased = true; endDraw(); } function
   stop(){endDraw();} function mouseEnter(e){ if(mouseReleased) return;
   var coords = getCoords(e,this); addCoords(coords.x,coords.y,true); }
   canvas.addEventListener("mousedown",press,false);
   canvas.addEventListener("mousemove",drag,false);
   document.addEventListener("mouseup",release);
   canvas.addEventListener("mouseout",stop,false);
   canvas.addEventListener("mouseenter",mouseEnter,false);
   canvas.addEventListener("touchstart",press,false);
   canvas.addEventListener("touchmove",drag,false);
   canvas.addEventListener("touchend",release,false);
   canvas.addEventListener("touchcancel",stop,false); } function
   init(conf){ canvas = document.createElement('canvas'); drawCtx =
   merge(conf,drawCtx,drawCtx); canvas.setAttribute('id',conf.id);
   canvas.setAttribute('width',conf.width);
   canvas.setAttribute('height',conf.height); if(conf.cssClass)
   canvas.setAttribute('class',conf.cssClass); if(conf.parent ===
   undefined) throw new TypeError('Erreur, par de parent pour le canvas
   !'); findElement(conf.parent).appendChild(canvas); if(typeof
   G_vmlCanvasManager !== "undefined") canvas =
   G_vmlCanvasManager.initElement(canvas); context =
   canvas.getContext("2d"); context.rect(0,0,conf.width,conf.height);
   context.clip(); mergeOn(context,drawCtx); context.beginPath();
   createListeners(); } this.getDrawing = function(){return coords;}
   this.exportDrawing = function(){ var i,paths=[],path=''; for(i=0;i 2){
