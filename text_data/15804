   function getMileageIcon(_mileages){ var intMileages =
   parseInt(_mileages,10); var strMileages = ""; var i=0; var intSuns = 0;
   var intMoons = 0; var intStars = 0; //每满100个积分升级一次;最多显示8个图标
   if(intMileages==0){ strMileages = ' [blank.png] ' } else{ intSuns =
   parseInt(intMileages/6400,10); //6400=8*8*100
   for(i=1;((i<=intSuns)&&(i<=8));i++){ strMileages += ' [blank.png] ' }
   intMileages = intMileages - intSuns*6400; if(intMileages>0 && i<8){
   intMoons = parseInt(intMileages/800,10);
   for(;((i<=intMoons)&&(i<=8));i++){ strMileages += ' [blank.png] ' } }
   intMileages = intMileages - intMoons*800; if(intMileages>0 && i<8){
   intStars = parseInt((intMileages-1)/100,10)+1;
   for(;((i<=intStars)&&(i<=8));i++){ strMileages += ' [blank.png] ' } } }
   return strMileages; } function login(){ var objUsername =
