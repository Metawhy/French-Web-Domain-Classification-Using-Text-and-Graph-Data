       function PixeoLog(Evenement, Valeur, RunWT1, RunLogmatic
           if (typeof(RunWT1)==='undefined') RunWT1 = true;____
           if (typeof(RunLogmatic)==='undefined') RunLogmatic =
   ____________________________________________________________
