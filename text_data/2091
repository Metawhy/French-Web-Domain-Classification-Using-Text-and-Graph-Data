   	function PixeoLogAny(Evenement, Valeur, OthersValue){______
   ____________________________________________________________
   		var DurationTime = parseFloat(new Date().getTime() - LogDu
   	___________________________________________________________
   		var wt1Value = {};________________________________________
   		wt1Value[Evenement] = Valeur;_____________________________
   		wt1Value["timeTaken"] = DurationTime;_____________________
   		__________________________________________________________
   		var LogMaticValue = {};___________________________________
   		LogMaticValue["timeTaken"] = DurationTime;________________
   		LogMaticValue["evt"] = Evenement;_________________________
   		LogMaticValue["value"] = Valeur;__________________________
   		__________________________________________________________
   		for(var key in OthersValue){______________________________
   			wt1Value[key] = OthersValue[key];________________________
   			LogMaticValue[key] = OthersValue[key];___________________
   		}_________________________________________________________
   		__________________________________________________________
   		_wt1Q.push(["trackEvent", wt1Value]);_____________________
   		__________________________________________________________
   		logmatic.log(Evenement + ' => ' + Valeur, LogMaticValue);_
   	}__________________________________________________________
