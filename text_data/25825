\u043e\u0431\u043b\u0430\u0441\u0442\u0438\u0442\u0435
\u0428\u0443\u043c\u0435\u043d, \u0421\u043b\u0438\u0432\u0435\u043d,
\u041c\u043e\u043d\u0442\u0430\u043d\u0430 \u0438
\u0421\u043e\u0444\u0438\u044f. \u0412\u0441\u0438\u0447\u043a\u0438
\u0436\u0435\u043b\u0430\u0435\u0449\u0438 \u0434\u0430 \u044f
\u043f\u043e\u0434\u043a\u0440\u0435\u043f\u044f\u0442
\u043c\u043e\u0433\u0430\u0442 \u0434\u0430 \u0433\u043e
