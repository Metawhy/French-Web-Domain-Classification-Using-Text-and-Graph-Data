   $("#viewgb").height(); var popupWidth = $("#viewgb").width();
   $("#viewgb").css({ "position": "absolute", "top":
   windowHeight/2-popupHeight/2, "left": windowWidth/2-popupWidth/2 });
   $("#backgroundPopup").css({ "height": "2000px" });
