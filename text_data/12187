   if(popupStatus==0){ $("#backgroundPopup").css({ "opacity": "0.7" });
   $("#backgroundPopup").fadeIn("slow"); $("#viewgb").fadeIn("slow");
   popupStatus = 1; } }function disablePopup(){ if(popupStatus==1){
   $("#backgroundPopup").fadeOut("slow"); $("#viewgb").fadeOut("slow");
   popupStatus = 0; } }function centerPopup(){ var windowWidth =
   document.documentElement.clientWidth; var windowHeight =
