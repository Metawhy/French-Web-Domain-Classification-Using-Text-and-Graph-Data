   var url = "/"; var urlmail = "http://www.monmetiercompagnons.com/"; var
   metier1 = ""; var metier2 = ""; var metier3 = ""; var etape = ""; var
   phase = ""; var metier = ""; var idselect = "";
   $(document).ready(function () { for(var i = 1; i <= 3; i++ ) {
   $('body').on("change", 'select[data-target="metier'+i+'"]',
   function(evt) { if($(this).attr("value") != undefined) { idselect =
   $(this).attr("data-target"); metier = $(this).attr("value");
   $('#'+idselect ).removeClass('metierselectOn'); ajaxMetier(metier,
   idselect, ''); } }); } $('body').on("click", '#lightbox_ajout_comp',
   function(evt) { if($(this).attr("data-id") != undefined) { metier =
   $(this).attr("data-id"); testNomMetier(metier); } });
   $('body').on("click", '#lightbox_choix_metier', function(evt) {
   choixmetier = $(this).parents().eq(1).attr("id"); if(choixmetier ==
   'metier1') { $('#etape_fim_metier').html(metier1[3]); $('#phase3
   #nomMetierEtape3').html(metier1[4]); textmail = metier1[5];
   //console.log(textmail); if($.jStorage.get('etape'))
   $.jStorage.deleteKey('etape'); $.jStorage.set('etape', Array('phase3',
   metier1[3], metier1[4], metier1[5] ) ); } if(choixmetier == 'metier2')
   { $('#etape_fim_metier').html(metier2[3]); $('#phase3
   #nomMetierEtape3').html(metier2[4]); textmail = metier2[5];
   //console.log(textmail); if($.jStorage.get('etape'))
   $.jStorage.deleteKey('etape'); $.jStorage.set('etape', Array('phase3',
   metier2[3], metier2[4], metier2[5] ) ); } if(choixmetier == 'metier3')
   { $('#etape_fim_metier').html(metier3[3]); $('#phase3
   #nomMetierEtape3').html(metier3[4]); textmail = metier3[5];
   //console.log(textmail); if($.jStorage.get('etape'))
   $.jStorage.deleteKey('etape'); $.jStorage.set('etape', Array('phase3',
   metier3[3], metier3[4], metier3[5] ) ); } $('#phase1').hide();
   $('#phase2').hide(); $('#phase3').show(); }); $('body').on("click",
   '#choix_ce_metier', function(evt) { if($(this).attr("data-id") !=
   undefined) { metier = $(this).attr("data-id"); var jqxhr =
   $.getJSON(url+'metiers/json/'+metier, function(data) {
   if($.jStorage.get('etape')) $.jStorage.deleteKey('etape');
   $.jStorage.set('etape', Array('phase3', data["finhtml"],
   data["nomMetier"], data["mail"] ));
   $('#etape_fim_metier').html(data["finhtml"]); $('#phase3
   #nomMetierEtape3').html(data["nomMetier"]); textmail = data["mail"]; })
   $('#phase1').hide(); $('#phase2').hide(); $('#phase3').show();
   OpenFavoris(); } }); $('body').on("click", '#supprimer_choix',
   function(evt) { if($(this).parents().eq(2).attr("id") != undefined) {
   idcomparateur = $(this).parents().eq(2).attr("id");
   sup_cookie(idcomparateur, 'suppchoix'); } read_cookie(); });
   $('body').on("click", '#supprimer_choix2', function(evt) {
   if($(this).parents().eq(1).attr("id") != undefined) { idcomparateur =
   $(this).parents().eq(1).attr("id"); idcomparateur =
   idcomparateur.replace(/metier_lightbox_/ ,"");
   sup_cookie(idcomparateur, 'suppchoix2'); testNomMetier(metier,
   'choix2'); phase = "phase2"; } read_cookie(); }); $('body').on("click",
   '#modifier_mon_choix', function(evt) { $.jStorage.deleteKey('etape');
   phase = ""; read_cookie(); }); read_cookie();
   $(".l_close").click(function (){ $("#lightbox3").fadeOut();
   $("#overlay").fadeOut(); }); //envoi mail
   $("#envoi_mail").click(function(){ var valid = ''; var mail =
   $("#mail").val(); //console.log(textmail); var text = textmail; if
   (!mail.match(/^([a-z0-9._-]+@[a-z0-9._-]+\.[a-z]{2,4}$)/i)) { valid +=
   'Adresse email non valide'; } if (valid!='') {
   $("#response").fadeIn("slow"); $("#response").html(valid); } else { var
   datastr ='mail=' + mail + '&text=' + text;
   $("#response").css("display", "block"); $("#response").html("Message en
   cours d'envoi .... "); $("#response").fadeIn("slow"); setTimeout(send ,
   2000 , datastr ); } return false; }); }); function read_cookie() {
   if($.jStorage.get('metier1')) { metier1 = $.jStorage.get('metier1');
   ajaxMetier(metier1[0],'metier1', metier1[1] , metier1[2], metier1[3],
   metier1[4] ); } if($.jStorage.get('metier2')) { metier2 =
   $.jStorage.get('metier2'); ajaxMetier(metier2[0],'metier2', metier2[1]
   , metier2[2], metier2[3], metier2[4] ); } if($.jStorage.get('metier3'))
   { metier3 = $.jStorage.get('metier3'); ajaxMetier(metier3[0],'metier3',
   metier3[1] , metier3[2], metier3[3], metier3[4] ); }
   if($.jStorage.get('etape')) { etape = $.jStorage.get('etape');
   $('#etape_fim_metier').html(etape[1]); $('#phase3
   #nomMetierEtape3').html(etape[2]); textmail = etape[3];
   //console.log(etape); phase = etape[0]; } //console.log(etape);
   //console.log(metier1[0]+'m1'); //console.log(metier2[0]+'m2');
   //console.log(metier3[0]+'m3'); if(phase == "phase3") {
   $('#phase1').hide(); $('#phase2').hide(); $('#phase3').show(); } else
   if (phase == "phase2") { $('#phase1').hide(); $('#phase2').show();
   $('#phase3').hide(); } else if (metier1 == "" && metier2 == "" &&
   metier3 == "" ) { $('#phase1').show(); $('#phase2').hide();
   $('#phase3').hide(); } else { $('#phase1').hide(); $('#phase2').show();
   $('#phase3').hide(); } } function
   ajaxMetier(metier,idselect,datahtml,dataphtml ){ if(datahtml == "") {
   var jqxhr = $.getJSON(url+'metiers/json/'+metier, function(data) {
   $.jStorage.set(idselect, Array(metier, data["html"], data["phtml"],
   data["finhtml"], data["nomMetier"], data["mail"] )); if(idselect ==
   'metier1') metier1 = Array(metier, data["html"], data["phtml"],
   data["finhtml"], data["nomMetier"], data["mail"]); if(idselect ==
   'metier2') metier2 = Array(metier, data["html"], data["phtml"],
   data["finhtml"], data["nomMetier"], data["mail"]); if(idselect ==
   'metier3') metier3 = Array(metier, data["html"], data["phtml"],
   data["finhtml"], data["nomMetier"], data["mail"]);
   $('#'+idselect).html(data["html"]);
   $('#metier_lightbox_'+idselect).html(data["phtml"]);
   $('#metier_lightbox_'+idselect).show(); $('#'+idselect).fadeOut('fast',
   function() { $('#'+idselect).fadeIn('fast'); });
   $('select[data-target="'+idselect+'"]').val(data["nom"]); }) } else {
   $('#'+idselect).html(datahtml);
   $('select[data-target="'+idselect+'"]').val(metier);
   $('#metier_lightbox_'+idselect).html(dataphtml); } if(phase ==
   "phase3") { $('#phase1').hide(); $('#phase2').hide();
   $('#phase3').show(); } else if(phase == "phase2") {
   $('#phase1').hide(); $('#phase2').show(); $('#phase3').hide(); } else {
   $('#phase1').hide(); $('#phase2').show(); $('#phase3').hide(); } }
   function sup_cookie(idcomparateur, suppchoix) {
   $.jStorage.set(idcomparateur, ""); if (idcomparateur == "metier1")
   metier1 = ""; if (idcomparateur == "metier2") metier2 = ""; if
   (idcomparateur == "metier3") metier3 = ""; if(suppchoix == "suppchoix"
   )$('#'+idcomparateur).fadeOut('fast'); else
   $('#'+idcomparateur).html('');
   $('select[data-target="'+idcomparateur+'"]').val(0); if(suppchoix ==
   "suppchoix2" ) $('#metier_lightbox_'+idcomparateur).html(''); else
   $('#metier_lightbox_'+idcomparateur).html('');
   //$('#metier_lightbox_'+idcomparateur).fadeOut('fast'); } function
   testNomMetier(metier,choix) { if(metier1[0] == metier || metier2[0] ==
   metier || metier3[0] == metier ) { if(choix != 'choix2')
   AjoutMetier('Attention','Métier déjà ajouté à vos favoris.'); } else {
   if(metier1 == "") { ajaxMetier(metier, 'metier1', ''); if(choix !=
   'choix2') AjoutMetier(' ','Métier ajouté.'); } else if(metier2 == "") {
   ajaxMetier(metier, 'metier2', ''); if(choix != 'choix2') AjoutMetier('
   ','Métier ajouté.'); } else if(metier3 == "") { ajaxMetier(metier,
   'metier3', ''); if(choix != 'choix2') AjoutMetier(' ','Métier
   ajouté.'); } else { //metiertest = metier;
   $('#overlay').height($(document).height()); $("#lightbox2").css({ left:
   ($(window).width()-$("#lightbox2").width()) /2+"px", top:
   ($(window).height()-$("#lightbox2").height()) /2+"px" });
   $("#lightbox2").fadeIn(); $("#overlay").fadeIn(); } } //read_cookie();
   } function AjoutMetier(alertTitre,alertText) { $('#lightbox3
   .l_content').html(alertText); $('#lightbox3 .l_titre
   span').html(alertTitre); $('#overlay').height($(document).height());
   $("#lightbox3").css({ left: ($(window).width()-$("#lightbox3").width())
   /2+"px", top: ($(window).height()-$("#lightbox3").height()) /2+"px" });
   $("#lightbox3").fadeIn(); $("#overlay").fadeIn();
   $("#lightbox3").fadeIn().delay(2000).fadeOut();
   $("#overlay").fadeIn().delay(2000).fadeOut(); } function OpenFavoris(){
   $('#slide').removeClass('close');
   $("#comparateur").animate({'background-color' : '#FFFFFF', width :
   '990px', 'z-index': '995'}); $(".comparateur").css('margin-left' ,
   '21px'); $('#slide').css('display', 'block');
   $('#slide').addClass('open'); } function send(datastr){ $.ajax({ type:
   "POST", url: urlmail+"mail.php", data: datastr, cache: false, success:
   function(html){ $("#response").fadeIn("slow");
   $("#response").html(html);
   setTimeout('$("#response").fadeOut("slow")',2000); } }); }
