   var SPECIAL_DAYS = {}; function dateIsSpecial(year, month, day) { var m
   = SPECIAL_DAYS[month]; if (!m) return false; for (var i in m) if (m[i]
   == day) return true; return false; }; function dateChanged(calendar) {
   // Beware that this function is called even if the end-user only //
   changed the month/year. In order to determine if a date was // clicked
   you can use the dateClicked property of the calendar: if
   (calendar.dateClicked) { // OK, a date was clicked, redirect to
   /yyyy/mm/dd/index.php var y = calendar.date.getFullYear(); var m =
   calendar.date.getMonth()+1; // integer, 0..11 var d =
   calendar.date.getDate(); // integer, 1..31 if (m<10 && d<10){
   window.location =
   "/offres/gestion/events_752_d"+y+"-0"+m+"-0"+d+"d_non.html"; } else if
   (m>=10 && d<10){ window.location =
   "/offres/gestion/events_752_d"+y+"-"+m+"-0"+d+"d_non.html"; } else if
   (m<10 && d>=10){ window.location =
