curl -X DELETE "https://api.cloudflare.com/client/v4/certificates/32857853390226
8680212849205732770752308931942346" \
     -H "X-Auth-User-Service-Key: v1.0-e24fd090c02efcfecb4de8f4ff246fd5c75b48946
fdf0ce26c59f91d0d90797b-cfa33fe60e8e34073c149323454383fc9005d25c9b4c502c2f063457
ef65322eade065975001a0b4b4c591c5e1bd36a6e8f7e2d4fa8a9ec01c64c041e99530c2-07b9efe
0acd78c82c8d9c690aacb8656d81c369246d7f996a205fe3c18e9254a"

