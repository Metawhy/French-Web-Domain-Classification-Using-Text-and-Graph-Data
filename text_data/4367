    "min": {
      "storedKeys": 2,
      "storedBytes": 16
    },
    "max": {
      "storedKeys": 4,
      "storedBytes": 32
    },
    "totals": {
      "storedKeys": 6,
      "storedBytes": 48
    },
    "query": {
      "limit": 10000,
      "since": "2019-01-02T02:20:00Z",
      "until": "2019-01-02T03:20:00Z",
      "metrics": [
