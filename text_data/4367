        "storedKeys"
      ],
      "dimensions": [
        "namespaceId"
      ],
      "sort": [
        "+storedBytes",
        "-namespaceId"
      ],
      "filters": "namespaceId==a4e8cbb7-1b58-4990-925e-e026d40c4c64"
