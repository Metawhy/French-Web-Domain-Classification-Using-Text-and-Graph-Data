     -H "X-Auth-Key: c2547eb745079dac9320b638f5e225cf483cc5cfdda41" \
     -H "Content-Type: application/json" \
     --data '{"name":"Widget Corps Internal Applications","auth_domain":"test.cl
oudflareaccess.com","login_design":{"background_color":"#c5ed1b","text_color":"#
c5ed1b","logo_path":"https://example.com/logo.png"}}'

