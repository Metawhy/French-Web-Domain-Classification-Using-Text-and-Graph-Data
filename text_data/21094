   'values[registrationFormDefinition_phone]': { trigger: 'blur',
   validators: { callback: { callback: function(value, validator, $field)
   { var messages = []; var messagesEN = []; var errorOccured = false; if
   (!value.trim().match("[\\\\+]{0,1}[0-9\\\\(\\\\)\\- \\\\.]*$")) {
   messages.push("Veuillez indiquer votre numéro de téléphone. (chiffres
   seulement, sans espaces)"); messagesEN.push("please enter your phone
   number. (Numbers only, no spaces)"); errorOccured |= true; } if
   (errorOccured) { return { valid: false, message:
   messages.join("<br/>"), message_en: messagesEN } } else { return true;
