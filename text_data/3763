   lonDep=value[1]; if(value[0]=="lonArr") lonArr=value[1]; } } }
   linkUrl=linkUrl.replace("'",""); //Partie qui permet de garder en
   memoire les X Y du point cliqu par l'utilisateur et de passer en
   parametre l'id Geo var linkForAddress=""; var coordDep=""; var
   coordArr=""; if (Point.IdLink !=0) { linkForAddress="$$$"+Point.IdLink;
